<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m" xmlns:f="sap.ui.layout.form">
    <f:SimpleForm>
        <Label text="Description" />
        <Text text="{DESCRIPTION}" />
        <Label text="Supplier" />
        <Text text="{SUPPLIER_NAME}" />
        <Label text="Unit" />
        <Text text="{MEASURE_UNIT}" />
        <Label text="Cost" />
        <Text text="{PRICE} {CURRENCY_CODE}" />
    </f:SimpleForm>
    <f:SimpleForm title="Supplier Data">
        <Label text="Company Name" />
        <Text text="{To_Supplier/COMPANY_NAME}" />
        <Label text="Website" />
        <Text text="{To_Supplier/WEB_ADDRESS}" />
        <Label text="Address" />
        <Text text="{To_Supplier/BUILDING}" />
        <Label text="Location" />
        <Text text="{To_Supplier/CITY} , {To_Supplier/COUNTRY}" />
    </f:SimpleForm>
    
</core:FragmentDefinition>